ls=[]
n=3
m=2

for a in range(1,n+1):
	for b in range(1,n+1):
		for c in range(1,n+1):
			for d in range(1,n+1):
				for e in range(1,n+1):
					for f in range(1,n+1):
						ls.append([a,b,c,d,e,f]);

def getUnique(s):
	return len(set(s));

f=[i for i in ls if getUnique(i)==m]

print("Number of combinations total:"+str(len(ls)));
print("Number of combinations with "+str(m)+" unique numbers: "+str(len(f)));
print("Probability of winning: "+str(float(len(f))/float(len(ls))));
