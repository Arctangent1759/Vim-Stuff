package com.amadeus.amadeus;
import java.lang.Thread;
public class FrequencyAnalyzer implements Runnable{
	protected Thread t;
	public FrequencyAnalyzer(){
		this.t=new Thread(this);
		t.start();
	}

	@Override
	public void run(){
		try{
			while (true){
				System.out.println("Running");
				Thread.sleep(10);
			}
		}catch(InterruptedException e)
		{
			System.out.println("Amadeus Thread Interrupted");
		}
	}
}
