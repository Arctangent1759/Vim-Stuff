def  getArrayPlay( A1,  A2):
	out=0;
	if len(A1)==0 or len(A2)==0:
		return 0;
	for i in range(max(len(A1),len(A2))):
		out+=A1[i%len(A1)]*A2[i%len(A2)]
	if out > 2**31:
		return 2**31
	if out < -1*(2**31):
		return -1*(2**31)
	return out;

#print getArrayPlay([1,2,3],[3,2,1])
#print getArrayPlay([],[])
#print getArrayPlay([1],[])
#print getArrayPlay([1,2,3],[3,2])

def  getOversizedWords( W,  S):
	NUMERIC=["1","2","3","4","5","6","7","8","9","0"]
	ALPHA=["a","b","c","d","e","f","g","h","i","j","k","l","m","n","o","p","q","r","s","t","u","v","w","x","y","z"]


	if S < 3:
		S=3
	elif S > 2**7:
		S=2**7

	total=0
	currLength=0
	for i in range(min(len(W),2**10)):#Iterate through the string
		currChar=W[i].lower()#get the current char
		if (currChar in ALPHA) or (currLength > 0 and currChar in NUMERIC):#If the current character is alphanumeric
			currLength+=1
		else:#Not alphanumeric
			if (currLength>=S):
				total+=1
			currLength=0
	if currLength>=S:
		total+=1

	return total


print getOversizedWords("!#@$!@#$!@#a22$",3)
