		\item 
			\begin{enumerate}[1)]
				\item
					$gcf(574,323)$\\
					=$gcf(323,251)$\\
					=$gcf(251,72)$\\
					=$gcf(72,35)$\\
					=$gcf(35,2)$\\
					=$gcf(2,1)$\\
					=$gcf(1,0)$\\
					=$\boxed{1}$
				\item
					$gcf(27,5)=(1,-2,11)$\\
					$=gcf(5,2)=(1,1,-2)$\\
					$=gcf(2,1)=(1,0,1)$\\
					$=gcf(1,0)=(1,1,0)$\\
					The multiplicative inverse of 5 mod 27 is$\boxed{11}$
				\item
					$5x+26=3$ mod $27$\\
					$5x+26+1=3+1$ mod $27$\\
					$5x=4$ mod $27$\\
					$11*5x=11*4$ mod $27$\\
					$x=44$ mod $27$\\
					$\boxed{x=17\mbox{ mod }27}$
				\item
					False.\\
					$a,b,c \in \mathbb{Z}, c>0$\\
					$\neg\exists a^{-1}, aa^{-1}=1$\\
					\textbf{Claim: } $gcf(a,c)=0 \implies ax=b\mbox{ mod }c$ has no solution.\\
					Disproof by counterexmaple:\\
					Let $a=6, b=4, c=8$.\\
					$gcf(6,8)=gcf(6,2)=gcf(2,0)=2 \implies 6$ has no multiplicative inverse mod $8$.\\
					$6x=4 \mbox{ mod } 8$ holds true for x=2.\\
					True $\implies$ False\\
					$\therefore$ False.
			\end{enumerate}
		\newpage
		\item
			\begin{enumerate}[1)]
				\item
					$2^{2013}$ mod 11\\
					$=2^{183*11}$ mod 11\\
					$=(2^{183})^{11}$ mod 11\\
					$=2^{183}$ mod 11\\
					$=2^{16\times11+7}$ mod 11\\
					$=2^{16\times11}2^{7}$ mod 11\\
					$=(2^{16})^{11}2^{7}$ mod 11\\
					$=2^{16}2^{7}$ mod 11\\
					$=2^{11+5}2^{7}$ mod 11\\
					$=2^{11}2^{13}$ mod 11\\
					$=2\times2^2$ mod 11\\
					$=8$ mod 11\\
				\item
					General Proof by Induction:\\
					$2^{5^{n}}$ mod 11\\
					$=(2^{5^{n-2}})^{25}$ mod 11\\
					$=(2^{5^{n-2}})^{22+3}$ mod 11\\
					$=(2^{5^{n-2}})^{2\times11}(2^{5^{n-2}})^{3}$ mod 11\\
					$=((2^{5^{n-2}})^{2})^{11}(2^{5^{n-2}})^{3}$ mod 11\\
					$=(2^{5^{n-2}})^{2}(2^{5^{n-2}})^{3}$ mod 11\\
					$=(2^{5^{n-2}})^{2+3}$ mod 11\\
					$=(2^{5^{n-2}})^{5}$ mod 11\\
					$=2^{5^{n-1}}$ mod 11\\
					$2^{5^{2013}}$ mod 11\\
					$=2^{5^{1}}$ mod 11\\
					$=32$ mod 11\\
					$=\boxed{10}$ mod 11
			\end{enumerate}
		\newpage
		\item
			$1250=37x+43y$\\
			$1250-43y=37x$ mod 37\\
			$1250-43y=0$ mod 37\\
			$29-6y=0$ mod 37\\
			\begin{tabular}{|c|c||c||c|c|c||c|c|c|}
				\hline
				x  & y  & x//y & $d_i$ & $a_i$ & $b_i$ & $d_0$ & $b_0$ & $a_0$ \\ \hline
				43 & 37 & 1    & 1     & 1     & -6    & 1     & -6    & 7     \\ 
				37 & 6  & 6    & 1     & 0     & 1     & 1     & 1     & -6    \\ 
				6  & 1  & 6    & 1     & 1     & 0     & 1     & 0     & 1     \\ 
				1  & 0  & --   & --    & --    & --    & 1     & 1     & 0     \\
				\hline
			\end{tabular}\\
			$37-6=31$ mod 37\\
			$29=6y$ mod 37\\
			$29\times31=6y\times31$ mod 37\\
			$899=y$ mod 37\\
			$11=y$ mod 37\\
			$1250-43y=37x$\\
			$21=x$\\
			$\boxed{\mbox{21 bottles of tea and 11 bottles of juice.}}$\\
			\textbf{To prove that this solution is unique:}
			Assume for contradiction: $37a_1+43b_1=1250$ and $37a_2+43b_2=1250$\\
			$37(a_1-a_2)+43(b_1-b_2)=0$\\
			$a_1-a_2=\frac{-43(b_1-b_2)}{37}$\\
			$a_1\in \mathbb{Z} \wedge a_2\in \mathbb{Z} \implies a_1-a_2 \in \mathbb{Z}$\\
			$\implies b_1-b_2 = 37k_1, k_1 \in \mathbb{Z}$\\
			$\implies a_1-a_2 = 43k_2, k_2 \in \mathbb{Z}$\\
			We know that $(a_1,b_1)=(21,11)$\\
			$\implies 11-b_2=37k$\\
			$\implies b_2=11-37k$\\
			$\implies k=0$\\
			$\implies 21-a_2=43k$\\
			$\implies a_2=21-43k$\\
			$\implies k=0$\\
			Contradiction.\\
			$\therefore$ our solution must be unique.
		\newpage
		\item
			We shall approach this problem with Induction.\\
			Let $x=F(n+1),y=F(n)$\\
			\textbf{Base Case: }$gcd(F(0) + F(2), F(1) + F(3)) = gcd(0 + 1, 1 + 2) = gcd(1, 3) = 1$\\
			\textbf{Inductive Hypothesis: } $gcd(F(n+3)+F(n+1),F(n+2)+F(n))=1$\\
			$F(n+3)+F(n+1)=F(n+2)+2F(n+1)=3F(n+1)+F(n)=3x+y$\\
			$F(n+2)+F(n)=F(n+1)+2F(n)a=x+2y$\\
			\\
			$gcd(F(n+3)+F(n+1),F(n+2)+F(n))=1$\\
			$\implies gcd(3x+y,x+2y)=1$\\
			$\implies gcd(3x+y,-5x)=1$\\

			\textbf{Inductive Step: }\\
			\textbf{Claim: }$gcd(F(n+4)+F(n+2),F(n+3)+F(n+1))=1$\\
			$F(n+4)+F(n+2)=F(n+3)+2F(n+2)=F(n+1)+3F(n+2)=4F(n+1)+3F(n)=4x+3y$\\
			$F(n+3)+F(n+1)=F(n+2)+2F(n+1)=3F(n+1)+F(n)=3x+y$\\
			\\
			$gcd(F(n+4)+F(n+2),F(n+3)+F(n+1))$\\
			$=gcd(3x+y,4+3y)$\\
			$=gcd(3x+y,-5x)=1$\\
			By induction, $gcd(F(n+3)+F(n+1),F(n+2)+F(n))=1$
