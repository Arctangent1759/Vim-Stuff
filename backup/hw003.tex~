		\item
			$3^{32} \mod 100$\\
			$=9^{16} \mod 100$\\
			$=81^8 \mod 100$\\
			$=61^4 \mod 100$\\
			$=21^2 \mod 100$\\
			$=41 \mod 100$\\
			$=41$\\
		\newpage
	\end{enumerate}
