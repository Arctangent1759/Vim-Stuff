	public static int leftBinarySearch(int[] arr, int value){
		if (arr.length==0){
			return -1;
		}
		if (arr.length==1){
			if (arr[0]==value){
				return 0;
			}else{
				return -1;
			}
		}
		if (arr[arr.length/2]<value){
			int[] a = new int[arr.length/2+1-arr.length%2]
			for (int i = arr.length/2; i < arr.length; i++){
		
			}
			return arr.length/2+leftBinarySearch();
		}
	}
}
