package com.amadeus.amadeus;
import java.awt.Graphics2D;
import java.lang.Thread;
public class Amadeus implements Runnable{

	FrequencyAnalyzer f;
	Thread t;
	public Amadeus(){
		f = new FrequencyAnalyzer();
		t = new Thread(this);
		t.start();
	}

	public void run(){
		while (true){
			try{
				Thread.sleep(50);
			}catch(InterruptedException e){}
		}
	}

	public void paint(Graphics2D g){
	}

	public static void main(String[] args){
		Amadeus a = new Amadeus();
	}

}
