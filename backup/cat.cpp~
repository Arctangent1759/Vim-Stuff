#include <iostream>
#include <fstream>
#include <string>
using namespace std;

int main(int argc, char *argv[]){
    ifstream file(argv[1]);
    if (file.is_open()){
        string line;
        while (getline(file,line)){
            cout << line << endl;
        }
        file.close();
    }else{
        cout << "Error: File not found." << endl;
    }
    return 0;
}
