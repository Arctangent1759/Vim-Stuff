urls=$(curl $1 | grep 'data-sessionlink' | grep 'title=' | grep 'watch?' | gawk '{split($2,a,"\""); split(a[2],b,";"); print "http://www.youtube.com"b[1]}')
for i in $urls;
do
  echo $i;
done;
if [ $(youtube-dl -t --extract-audio --audio-format=mp3 asdf) ];
then
  echo "success";
else
  echo "failure";
fi;
