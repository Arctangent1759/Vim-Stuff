def crypt(s):
    out={"len":len(s)}
    for i in range(len(s)):
        if s[i] not in out:
            out[s[i]]=[i]
        else:
            out[s[i]].append(i)
    return out

def decrypt(d):
    out=[""]*d['len']
    for k in d:
        if k=='len':
            continue
        for i in d[k]:
            out[i]=k
    return "".join(out)

print decrypt({'&': [97, 138, 166], '-': [25, 27, 35, 39, 44, 52, 55, 62, 68, 71], '/': [5, 6, 18], '.': [14], '1': [72, 87, 96], '0': [77, 79, 94], '3': [88, 90, 93], '5': [74, 81], '4': [73], '7': [76, 80, 91, 92], '9': [75, 78, 95], '8': [89], ':': [4], '=': [86, 110, 149, 177], '?': [82], 'len': 188, '_': [101, 121, 129, 142, 157, 170], 'a': [26, 29, 34, 48, 103, 106, 115, 131, 159, 182], 'c': [15, 47, 56, 102, 113, 132, 147, 160, 180], 'b': [45, 134, 162], 'e': [21, 23, 46, 51, 61, 65, 67, 84, 133, 148, 161, 172], 'd': [12, 31, 127, 155, 173], 'g': [7, 108, 122, 150], 'f': [36, 54, 117, 130, 158, 184], 'i': [8, 69, 107, 114, 123, 151, 174, 181], 'h': [0, 20, 64], 'k': [137, 165], 'm': [10, 17, 100, 104, 125, 141, 153, 169, 171, 176], 'l': [116, 118, 183, 185], 'o': [11, 13, 16, 32, 37, 41, 53, 57, 112, 119, 126, 128, 135, 136, 144, 154, 156, 163, 164, 179, 186], 'n': [30, 43, 109], 'p': [3, 28, 40, 105], 's': [24, 50, 60, 70, 111, 143, 178], 'r': [22, 33, 38, 42, 59, 66, 83, 146], 'u': [49, 58, 98, 139, 145, 167, 175], 't': [1, 2, 19, 63, 99, 140, 168], 'w': [120, 187], 'v': [85], 'z': [9, 124, 152]})
