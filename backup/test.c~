#include <stdio.h>

typedef struct node {
	int var1;
	int var2;
} point;

int sum_node_eles(point *my_node) {  //returns the sum of members of a single node
	int total;
	total += my_node->var1 + my_node->var2;
	return total;
}

int main(void) {
	struct node *arr[5];
	struct node pt1 = {1,2};
	arr[0] =  &pt1;
	printf("The sum is %d.\n", sum_node_eles(*arr));
	return 0;
}
