from random import randint;

NUM_SIMS=1000;
MAX_VOLUNTEERS=1000
TARGET=1.0/3.0

def simulate(num_volunteers):
	success=0.0;
	for _ in range(NUM_SIMS):
		if sim(num_volunteers):
			success+=1.0;
	return float(success)/float(NUM_SIMS);


def sim(num_volunteers):
	hasHug = [(False if i < 250 else True) for i in range(450)];
	for _ in range(num_volunteers):
		hasHug[randint(0,449)]=True;
	return not (False in hasHug)

print simulate(2300);
