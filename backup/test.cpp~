

using namespace std;
#include <iostream>
#include "lists.h"

ListNode* FromInput (istream &is) {
	//int k;
	ListNode* head = 0;
	while (is >> k) {
		head = new ListNode (k, head);
	}
	return head;
}

int main () {
	ListNode* list = FromInput (cin);
	list->Print ();
	if (list) {
		delete list;
	}
	return 0;
}

