		\item
			\begin{enumerate}[a)]
				\item
					$2^5=2\times4^2=2\times16=32$ mod 105
				\item
					$3^5=3\times9^2=3\times81=243=33$ mod 105
				\item$ $\\
					$2\times4\times6=48$\\
					\begin{tabular}{|c|c||c||c|c|c||c|c|c|}
						\hline
						x  & y &x//y& d & a & b & d & b & a\\\hline
						48 & 5 &  9 & 1 & -1& 2 & 1 & 2 &-19\\\hline
						5  & 3 &  1 & 1 & 1 & -1& 1 & -1& 2\\\hline
						3  & 2 &  1 & 1 & 0 & 1 & 1 & 1 &-1\\\hline
						2  & 1 &  2 & 1 & 1 & 0 & 1 & 0 & 1\\\hline
						1  & 0 &  - & - & - & - & 1 & 1 & 0\\\hline
					\end{tabular}\\
					48-19=\boxed{29}
				\item
					$D(E(x))=(x^e)^d=x^{ed}$ mod N\\
					$=(x^e)^d=x^{ed}$ mod $(p)(q)(r)$\\
					$ed=1$ mod $(p-1)(q-1)(r-1)$\\
					By Fermat's Little Theorem:\\
					$x^{ed}=x^{ed\mbox{ mod }(p-1)(q-1)(r-1)}$ mod $(p)(q)(r)$\\
					$x^{ed}=x^{1}$ mod $(p)(q)(r)$\\
					$x^{ed}=x$\\
					By definition of inverse.
			\end{enumerate}

	\end{enumerate}
