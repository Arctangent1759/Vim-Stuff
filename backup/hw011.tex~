		\item 
			\begin{enumerate}[1.]
				\item
					$\sigma^2=np(1-p)$\\
					$p=\frac{1}{2}$\\
					$P(|X-\mu|\ge k\sigma)\le\frac{1}{k^2}$\\
					$P(|X-\mu|\ge k\sqrt{np(1-p)})\le\frac{1}{k^2}$\\
					$k\sqrt{np(1-p)}=\sqrt{n}$\\
					$k\sqrt{\frac{1}{4}}=1$\\
					$k=2$\\
					$P(|X-\mu|\ge \sqrt{n})\le\frac{1}{2^2}=\boxed{\frac{1}{4}}$\\
					$k\sqrt{np(1-p)}=5\sqrt{n}$\\
					$k\sqrt{\frac{1}{4}}=5$\\
					$k=10$\\
					$P(|X-\mu|\ge 5\sqrt{n})\le\frac{1}{2^2}=\boxed{\frac{1}{100}}$\\
				\item
					$\sigma^2=1$\\
					$P(|X-\mu|\ge k\sigma)\le\frac{1}{k^2}$\\
					$k\sigma=10$
					$k=10$
					$P(|X-\mu|\ge k\sigma)\le\boxed{\frac{1}{100}}$\\
			\end{enumerate}
		\newpage
		\item 
			\textbf{Prints the approximate value of pi (3.1416128)}:
\begin{verbatim}
from random import random

NUMPOINTS=10000000;
in_circle=0;

for _ in range(NUMPOINTS):
    point=(2*random()-1,2*random()-1)
    if (point[0]**2+point[1]**2)**.5<=1:
        in_circle+=1

print ((float(in_circle)/float(NUMPOINTS)))*4
\end{verbatim}
			\textbf{Error graph ($\pi-f(x)$)}:\\
			\includegraphics[width=4in]{Plot.png}
		\newpage
		\item 
			\begin{enumerate}[1.]
				\item
                  \textbf{true}.\\
                  $E[XY]=\sum_{i=0}^{\infty}\sum_{j=0}^{\infty}ijP(X=i \cap Y=j)$\\
                  $E[XY]=\sum_{i=0}^{\infty}\sum_{j=0}^{\infty}ijP(X=i)P(Y=j)$\\
                  $E[X]E[Y]=(\sum_{i=0}^{\infty}iP(X=i))(\sum_{j=0}^{\infty}jP(Y=j))$\\
                  $E[X]E[Y]=\sum_{i=0}^{\infty}\sum_{j=0}^{\infty}ijP(X=i)P(Y=j)$\\
                  $E[XY]=E[X]E[Y]$\\
                  $P(X=i)P(Y=j)=P(X=i \cap Y=j)$
                  X and Y are independent. QED.
				\item
                  \textbf{true}.\\
                  $E[XY]=\sum_{i=0}^{\infty}\sum_{j=0}^{\infty}ijP(X=i \cap Y=j)$\\
                  $E[XY]=\sum_{i=0}^{\infty}\sum_{j=0}^{\infty}ijP(X=i)P(Y=j)$\\
                  $E[XY]=(\sum_{i=0}^{\infty}iP(X=i))(\sum_{j=0}^{\infty}jP(Y=j))$\\
                  $E[XY]=E[X]E[Y]$\\
                  QED.
				\item
                  \textbf{false}.\\
                  Let $P(Y=j|X=i)=2P(X=i)$\\
                  $E[XY]=\sum_{i=0}^{\infty}\sum_{j=0}^{\infty}ijP(X=i \cap Y=j)$\\
                  $E[XY]=\sum_{i=0}^{\infty}\sum_{j=0}^{\infty}ijP(X=i)P(Y=j|X=i)$\\
                  $E[XY]=2\sum_{i=0}^{\infty}\sum_{j=0}^{\infty}ijP(X=i)P(Y=j)$\\
                  $E[XY]=2\sum_{i=0}^{\infty}iP(x=i)\sum_{j=0}^{\infty}jP(Y=j)$\\
                  $E[XY]=2E[X]E[Y]$
				\item
                  \textbf{false}.\\
                  Let p=3.\\
                  Let X be a random variable denoting the amount of the roll (in 0,1,2).\\
                  Let Y be the same as X.\\
                  $E[S]=3.333$\\
                  $E[X]+E[Y]=2+2=4$\\
                  $E[S]\neq E[X]+E[Y]$\\
			\end{enumerate}
