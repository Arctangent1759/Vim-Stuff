\section*{1.}
	I understand the course policies.

\section*{2.}

\begin{itemize}
	\item[(a)]
		$f=\Omega(g)$. For $c=1$, $f(n)>cg(n) \forall n > 1$.
	\item[(b)]
		$f=\Theta(g)$. Drop lower order terms to get the same function.
	\item[(c)]
		$f=\Theta(g)$. Drop lower order terms to get the same function.
	\item[(d)]
		$f=\Theta(g)$. $500n = \Theta(8n)$.
	\item[(e)]
		$f=\Theta(g)$. $\log(n^7)=7\log(n)$, and $5\log(n) = \Theta(7\log(n))$.
	\item[(f)]
		$f=\Theta(g)$. $5n\log(5n)=5n(\log(5)+\log(n))=5(\log(5))+5n\log(n)=\Theta(n\log(n))$
	\item[(g)]
		$f=O(g)$. For $c=1$, $cf(n)<g(n) \forall n > 2$.
	\item[(h)]
		$f=O(g)$. Simplifies to $log(n)<n^.002$, and a polynomial always scales faster than a logarithm.
	\item[(i)]
		$f=O(g)$. Simplifies to $log(n)<n^.0001$, and a polynomial always scales faster than a logarithm.
	\item[(j)]
		$f=\Omega(g)$. Simplifies to $n^{\frac{1}{6}}>log(n)$, and a polynomial always scales faster than a logarithm.
	\item[(k)]
		$f=O(g)$. Let $u=\log(n)$. The expressions become $f(u)=u10^u$ and $g(u)=u^u$, in which g grows faster.
	\item[(l)]
		$f=O(g)$. Let $u=\log_{2}(n)$. The expressions become $f(u)=2^{\frac{u}{3}}$ and $g(u)=3^u$, in which g grows faster.
	\item[(m)]
		$f=\Omega(g)$. For $c=1$, $5^n > n4^n \forall n \ge 15$.
	\item[(n)]
		$f=O(g)$. $2^{3n}=(2^3)^n=8^n>2^n \forall n > 1$
	\item[(o)]
		$f=\Theta(g)$. $7^n=7*7^{n-1}=\Theta(7^{n-1})$
	\item[(p)]
		$f=O(g)$. Let $u=\log(n)$. The expressions become $f(u)=u^u$ and $g(u)=7^{u^2}$, in which g grows faster, as $u^u < 7^{u^2} \forall n > 2$.
	\item[(q)]
		$f=O(g)$. For $c=1$, $8^n < n$! for $n \ge 21$
\end{itemize}

\section*{3.}

\begin{itemize}
	\item[(a)]
		$\sum_{i=1}^n i^k < \sum_{i=1}^n n^k = n*n^k=n^{k+1}$\\
		$\Rightarrow \sum_{i=1}^n i^k = O(n^{k+1})$\\
		$\sum_{i=1}^n i^k < \sum_{i=\frac{n}{2}}^n \frac{n}{2}^k = \frac{n}{2}*\frac{n}{2}^k=\frac{1}{2}^{k+1}n^{k+1}$\\
		$\Rightarrow \sum_{i=1}^n i^k = \Omega(n^{k+1})$\\
		$\Rightarrow \sum_{i=1}^n i^k = \Theta(n^{k+1})$\\
	\item[(b)]
		$log(n!)=\sum_{k=1}^{n} \log(k) \le \sum_{k=1}^{n} \log(n) = n\log(n)$
		$\Rightarrow \log(n!) = O(n\log(n))$\\
		$n! = (n)(n-1)\cdots(2)(1) > (\frac{n}{2})(\frac{n}{2})(\frac{n}{2})\cdots(\frac{n}{2}-1)\cdots(2)(1)$\\
		$=\frac{n}{2}^\frac{n}{2}(\frac{n}{2}-1)! $\\
		$\log(\frac{n}{2}^\frac{n}{2})=\frac{n}{2}\log(\frac{n}{2})=\frac{1}{2}n\log(n)-\frac{1}{2}n\log(2)=\Omega(n\log(n))$\\
		$\Rightarrow log(n!)=\Omega(n\log(n))$\\
		$\Rightarrow log(n!)=\Theta(n\log(n))$\\
	\item[(c)]
		$\lim_{n\rightarrow \infty} \sum_{i=0}^n \frac{1}{i} < \lim_{n\rightarrow \infty} \sum_{i=0}^n \frac{1}{2^{floor{\log(i)}}}$
		$\lim_{n\rightarrow \infty} \sum_{i=0}^n \frac{1}{2^{floor{\log(i)}}}$ is a reimann sum for the integral of $\frac{1}{x ln(2)}$.\\
		Hence, $\lim_{n\rightarrow \infty} \sum_{i=0}^n \frac{1}{i} <  \lim_{n\rightarrow \infty} \ln(2)\log(n)=O(\log(n))$
		$\lim_{n\rightarrow \infty} \sum_{i=0}^n \frac{1}{i} > \lim_{n\rightarrow \infty} \sum_{i=0}^n \frac{1}{2^{ceil{\log(i)}}}$
		$\lim_{n\rightarrow \infty} \sum_{i=0}^n \frac{1}{2^{ceil{\log(i)}}}$ is a reimann sum for the integral of $\frac{1}{x ln(2)}$.\\
		Hence, $\lim_{n\rightarrow \infty} \sum_{i=0}^n \frac{1}{i} >  \lim_{n\rightarrow \infty} \ln(2)\log(n)=\Omega(\log(n))$
\end{itemize}

\section*{4.}

\begin{itemize}
	\item[(a)]
		$c<1 \Rightarrow \lim_{n\rightarrow \infty} \sum_{k=0}^{k=n}c^k=1+\frac{1}{1-c}=\Theta(1)$
	\item[(b)]
		$c=1 \Rightarrow \sum_{k=0}^{k=n}1^k=\sum_{k=0}^{k=n}1=n=\Theta(n)$
	\item[(c)]
		$c>1 \Rightarrow \lim_{n\rightarrow \infty} \sum_{k=0}^{k=n}c^k=c^n+c^{n-1}+c^{n-2}+c^{n-3}+c^{n-4}+\cdots+1$\\
		$=(1+\frac{1}{c}+\frac{1}{c}^2+\cdots+\frac{1}{c}^n)(c^n)$\\
		$=(\sum_{k=0}^{k=n} (\frac{1}{c})^k) (c^n)$\\
		$=Cc^n$ (The sum of a geometric series is a finite number)\\
		$=\Theta(c^n)$
\end{itemize}

\section*{5.}
	\textbf{Base Case:}$GCD(1,1)=1$\\
	\textbf{Inductive Hypothesis:}$GCD(F_{n+1},F_{n})=1$\\
	\textbf{Inductive Step:}$GCD(F_{n+2},F_{n+1})=GCD(F_{n+1}+F_{n},F_{n+1})=GCD(F_{n+1},F_{n})=1$\\
	By induction, $GCD(F_{n+1},F_{n})=1$.
\section*{6.}
	$GCD(a,b) = 1 = GCD(b,a)$\\
	b has in inverse mod a if and only if they are relatively prime.\\
	Two numbers are relatively prime if they have a gcd of 1.\\
	We are given that a has an inverse mod b, so $GCD(a,b)=1$.\\
	$GCD(a,b) = 1 = GCD(b,a)$\\
	Hence, b is relatively prime to a.\\
	So b has an inverse mod a.
