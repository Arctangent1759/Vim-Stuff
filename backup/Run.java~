public class Run{
	int occupant;
	int starveTime;
	int runLength;

	public Run(int occupant,int runLength){
		this.occupant=occupant;
		this.starveTime=0;
		this.runLength=runLength;
	}
	public Run(int occupant,int runLength,int starveTime){
		this.occupant=occupant;
		this.starveTime=starveTime;
		this.runLength=runLength;
	}
	public String toString(){
		return "{o:"+occupant+", r:"+runLength+", s:"+starveTime+"}";
		//return "{occupant: "+occupant+", runLength: "+runLength+", starveTime: "+starveTime+"}";
	}
	public boolean equals(Run other){
		if (this.occupant==Ocean.SHARK){
			return (this.occupant == other.occupant && this.starveTime == other.starveTime);
		}else{
			return (this.occupant == other.occupant);
		}
	}
}
