import list.*;
public class BadCode{
	public static void main(String[] args){
		DList d = new DList();
		DList e = new DList();
		d.insertBack(1);
		d.insertBack(2);
		d.insertBack(3);
		e.insertBack(4);
		e.insertBack(5);
		e.insertBack(6);
		e.insertBack(7);
		System.out.println("==============");
		System.out.println("Initial State:");
		System.out.println("==============");
		System.out.println("Contents of DList d: " + d);
		System.out.println("Length of DList d: " + d.length());
		System.out.println("Contents of DList e: " + e);
		System.out.println("Length of DList e: " + e.length());

		System.out.println();
		System.out.println("Obtaining second element of d...");
		System.out.println();

		DListNode n = d.next(d.front());

		System.out.println("calling e.remove on n");
		System.out.println();

		e.remove(n);

		System.out.println("============");
		System.out.println("Final State:");
		System.out.println("============");

		System.out.println("Contents of DList d: " + d);
		System.out.println("Length of DList d: " + d.length());
		System.out.println("Contents of DList e: " + e);
		System.out.println("Length of DList e: " + e.length());

		System.out.println("Violation of invariannt #6; length does not equal the number of nodes reachable from the sentinel node.");
	}
}
