<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<title>Sample</title>
		<script src="http://code.jquery.com/jquery-1.9.1.js"></script>
		<script src="https://raw.github.com/padolsey/jQuery-Plugins/master/cross-domain-ajax/jquery.xdomainajax.js"></script>
	</head>
	<body>
		<script>
			function getRawHtml(url,onLoad,onFail=function(){}){
				var data=null;
				$.ajax({
					url: url,
					type: 'GET',
					success: function(res) {
						data = res.responseText;
						onLoad(toLineArray(data));
					},
					failure: function(res){
						data="FAILURE";
						onFail(data);
					}
				});
			}
			function toLineArray(data){
				return data.split("\n");
			}
		</script>
	</body>
</html>
