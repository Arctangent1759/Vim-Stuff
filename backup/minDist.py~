def minDist(pts):
	pts=sorted(pts,key=lambda x:x[0])
	if len(pts)==2:
		return ((pts[0][0]-pts[1][0])**2+(pts[0][1]-pts[1][1])**2)**.5
	for i in range(len(pts)):
		if len(pts[:i])==len(pts[i:]):
			left=pts[:i]
			right=pts[i:]
			x=pts[i][0]
			break;
	dL=minDist(left)
	dR=minDist(right)
	d=min(dL,dR)
	cPts=[]
	for i in pts:
		if abs(i[0]-pts)<d:
			cPts.append(i)
	dM=-1
	cPts=sorted(cPts,key=lambda x:x[1])
	for i in range(len(cPts)):
