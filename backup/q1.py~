count = int(raw_input())
items={}
for _ in range(count):
    item=raw_input()
    if items.has_key(item):
        items[item]+=1
    else:
        items[item]=1
itemList = list(reversed(sorted(reversed(sorted([productName for productName in items])), key=lambda productName: items[productName])))
for item in itemList[:3]:
    print item
